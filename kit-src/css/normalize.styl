html, body,
h1, h2, h3, h4, h5, h6 {
	margin: 0;
	padding: 0;
	display: block;
}
img {
	border: none;
	display: inline-block;
	vertical-align: bottom;
}
/*
* Make html take up the entire screen
* Then set touch-action to avoid touch delay on mobile IE
*
* Make body take up the entire screen
* Remove body margin so layout containers don't cause extra overflow.
*/
html, body {
	position: relative;
	min-height: 100%;
	width: 100%;
	-ms-touch-action: manipulation;
	touch-action: manipulation;
}
html {
}
body {
	overflow-y: scroll;
}
h1, h2, h3, h4, h5, h6, .title_h1, .title_h2, .title_h3, .title_h4 {
	line-height: 1;
}
h1, .title_h1 {
	font-weight: bold;
	font-size: 25pt;
	margin: 0 0 20px;
}
h2, .title_h2  {
	font-weight: bold;
	font-size: 18pt;
	margin: 0 0 10px;
}
h3, .title_h3  {
	font-weight: bold;
	font-size: 15pt;
	margin: 0 0 5px;
}
h4, h5, h6, .title_h4  {
	font-weight: bold;
	font-size: 12pt;
	margin: 0 0 5px;
}

p {
	margin: 0 0 10px;
}

b, strong {
	font-weight: 700;
}
address {
	font-style: normal;
	font-weight: normal;
}

ul, ol {
	margin: 10px 0;
	padding: 0 0 0 30px;
}

input, textarea, select, option {
	margin: 0;
	outline: 0;
	box-shadow: none;
	vertical-align: top;
}